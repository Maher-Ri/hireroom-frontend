{
  "$schema": "./node_modules/@biomejs/biome/configuration_schema.json",
  "extends": [
    "ultracite/core"
  ],
  "linter": {
    "rules": {
      "nursery": {
        "useSortedClasses": "off"
      },
      "suspicious": {
        "noConsole": "off",
        "noExplicitAny": "off"
      }
    }
  },
  "assist": {
    "actions": {
      "source": {
        "useSortedAttributes": "off"
      }
    }
  }
}